{
  "name": "express-es6-rest-api",
  "version": "0.0.1",
  "description": "ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "NODE_ENV=test eslint src && mocha"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "debug": "^2.6.8",
    "dotenv": "^4.0.0",
    "express": "^4.13.3",
    "joi": "^10.5.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.10.3",
    "morgan": "^1.8.0",
    "resource-router-middleware": "^0.6.0",
    "short-uuid": "^2.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.0.0",
    "eslint": "^3.1.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.9.2",
    "sinon": "^2.3.2"
  }
}
